                          <label
                            htmlFor="financial-analysis"
                            className="font-medium text-gray-700 cursor-pointer"
                          >
                            Financial Analysis
                          </label>
                          <p className="text-gray-500">
                            Detailed financial metrics and trends
                          </p>
                        </div>
                      </div>
                      <div className="relative flex items-start">
                        <div className="flex items-center h-5">
                          <input
                            id="provider-performance"
                            name="report-template"
                            type="radio"
                            className="focus:ring-indigo-500 h-4 w-4 text-indigo-600 border-gray-300 cursor-pointer"
                            checked={
                              selectedReportTemplate === "provider-performance"
                            }
                            onChange={() =>
                              setSelectedReportTemplate("provider-performance")
                            }
                          />
                        </div>
                        <div className="ml-3 text-sm">
                          <label
                            htmlFor="provider-performance"
                            className="font-medium text-gray-700 cursor-pointer"
                          >
                            Provider Performance
                          </label>
                          <p className="text-gray-500">
                            Analysis of claims by healthcare provider
                          </p>
                        </div>
                      </div>
                      <div className="relative flex items-start">
                        <div className="flex items-center h-5">
                          <input
                            id="processing-efficiency"
                            name="report-template"
                            type="radio"
                            className="focus:ring-indigo-500 h-4 w-4 text-indigo-600 border-gray-300 cursor-pointer"
                            checked={
                              selectedReportTemplate === "processing-efficiency"
                            }
                            onChange={() =>
                              setSelectedReportTemplate("processing-efficiency")
                            }
                          />
                        </div>
                        <div className="ml-3 text-sm">
                          <label
                            htmlFor="processing-efficiency"
                            className="font-medium text-gray-700 cursor-pointer"
                          >
                            Processing Efficiency
                          </label>
                          <p className="text-gray-500">
                            Metrics on claim processing times and bottlenecks
                          </p>
                        </div>
                      </div>
                      <div className="relative flex items-start">
                        <div className="flex items-center h-5">
                          <input
                            id="custom-report"
                            name="report-template"
                            type="radio"
                            className="focus:ring-indigo-500 h-4 w-4 text-indigo-600 border-gray-300 cursor-pointer"
                            checked={selectedReportTemplate === "custom-report"}
                            onChange={() =>
                              setSelectedReportTemplate("custom-report")
                            }
                          />
                        </div>
                        <div className="ml-3 text-sm">
                          <label
                            htmlFor="custom-report"
                            className="font-medium text-gray-700 cursor-pointer"
                          >
                            Custom Report
                          </label>
                          <p className="text-gray-500">
                            Build a report with custom fields and metrics
                          </p>
                        </div>
                      </div>
                    </div>
                  </div>
                  {/* Middle Column: Report Parameters */}
                  <div className="md:col-span-1">
                    <h4 className="text-sm font-medium text-gray-900 mb-4">
                      Report Parameters
                    </h4>
                    <div className="space-y-4">
                      <div>
                        <label
                          htmlFor="report-name"
                          className="block text-sm font-medium text-gray-700 mb-1"
                        >
                          Report Name
                        </label>
                        <input
                          type="text"
                          name="report-name"
                          id="report-name"
                          className="shadow-sm focus:ring-indigo-500 focus:border-indigo-500 block w-full sm:text-sm border-gray-300 rounded-md"
                          placeholder="Enter report name"
                        />
                      </div>
                      <div>
                        <label
                          htmlFor="report-description"
                          className="block text-sm font-medium text-gray-700 mb-1"
                        >
                          Description (Optional)
                        </label>
                        <textarea
                          id="report-description"
                          name="report-description"
                          rows={3}
                          className="shadow-sm focus:ring-indigo-500 focus:border-indigo-500 block w-full sm:text-sm border border-gray-300 rounded-md"
                          placeholder="Enter report description"
                        ></textarea>
                      </div>
                      <div>
                        <label
                          htmlFor="report-grouping"
                          className="block text-sm font-medium text-gray-700 mb-1"
                        >
                          Group By
                        </label>
                        <select
                          id="report-grouping"
                          name="report-grouping"
                          className="mt-1 block w-full pl-3 pr-10 py-2 text-base border-gray-300 focus:outline-none focus:ring-indigo-500 focus:border-indigo-500 sm:text-sm rounded-md"
                        >
                          <option>Status</option>
                          <option>Claim Type</option>
                          <option>Provider</option>
                          <option>Insurance</option>
                          <option>Date (Weekly)</option>
                          <option>Date (Monthly)</option>
                        </select>
                      </div>
                      <div>
                        <label className="block text-sm font-medium text-gray-700 mb-1">
                          Include Charts
                        </label>
                        <div className="mt-2 space-y-2">
                          <div className="flex items-center">
                            <input
                              id="charts-pie"
                              name="charts-pie"
                              type="checkbox"
                              className="h-4 w-4 text-indigo-600 focus:ring-indigo-500 border-gray-300 rounded cursor-pointer"
                              defaultChecked
                            />
                            <label
                              htmlFor="charts-pie"
                              className="ml-2 text-sm text-gray-700 cursor-pointer"
                            >
                              Pie/Donut Charts
                            </label>
                          </div>
                          <div className="flex items-center">
                            <input
                              id="charts-bar"
                              name="charts-bar"
                              type="checkbox"
                              className="h-4 w-4 text-indigo-600 focus:ring-indigo-500 border-gray-300 rounded cursor-pointer"
                              defaultChecked
                            />
                            <label
                              htmlFor="charts-bar"
                              className="ml-2 text-sm text-gray-700 cursor-pointer"
                            >
                              Bar/Column Charts
                            </label>
                          </div>
                          <div className="flex items-center">
                            <input
                              id="charts-line"
                              name="charts-line"
                              type="checkbox"
                              className="h-4 w-4 text-indigo-600 focus:ring-indigo-500 border-gray-300 rounded cursor-pointer"
                              defaultChecked
                            />
                            <label
                              htmlFor="charts-line"
                              className="ml-2 text-sm text-gray-700 cursor-pointer"
                            >
                              Line/Area Charts
                            </label>
                          </div>
                          <div className="flex items-center">
                            <input
                              id="charts-tables"
                              name="charts-tables"
                              type="checkbox"
                              className="h-4 w-4 text-indigo-600 focus:ring-indigo-500 border-gray-300 rounded cursor-pointer"
                              defaultChecked
                            />
                            <label
                              htmlFor="charts-tables"
                              className="ml-2 text-sm text-gray-700 cursor-pointer"
                            >
                              Data Tables
                            </label>
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                  {/* Right Column: Export Options */}
                  <div className="md:col-span-1">
                    <h4 className="text-sm font-medium text-gray-900 mb-4">
                      Export Options
                    </h4>
                    <div className="space-y-4">
                      <div>
                        <label className="block text-sm font-medium text-gray-700 mb-1">
                          Format
                        </label>
                        <div className="mt-2 space-y-2">
                          <div className="flex items-center">
                            <input
                              id="format-pdf"
                              name="format"
                              type="radio"
                              className="focus:ring-indigo-500 h-4 w-4 text-indigo-600 border-gray-300 cursor-pointer"
                              checked={selectedExportFormat === "pdf"}
                              onChange={() => setSelectedExportFormat("pdf")}
                            />
                            <label
                              htmlFor="format-pdf"
                              className="ml-2 text-sm text-gray-700 cursor-pointer"
                            >
                              PDF Document
                            </label>
                          </div>
                          <div className="flex items-center">
                            <input
                              id="format-excel"
                              name="format"
                              type="radio"
                              className="focus:ring-indigo-500 h-4 w-4 text-indigo-600 border-gray-300 cursor-pointer"
                              checked={selectedExportFormat === "excel"}
                              onChange={() => setSelectedExportFormat("excel")}
                            />
                            <label
                              htmlFor="format-excel"
                              className="ml-2 text-sm text-gray-700 cursor-pointer"
                            >
                              Excel Spreadsheet
                            </label>
                          </div>
                          <div className="flex items-center">
                            <input
                              id="format-csv"
                              name="format"
                              type="radio"
                              className="focus:ring-indigo-500 h-4 w-4 text-indigo-600 border-gray-300 cursor-pointer"
                              checked={selectedExportFormat === "csv"}
                              onChange={() => setSelectedExportFormat("csv")}
                            />
                            <label
                              htmlFor="format-csv"
                              className="ml-2 text-sm text-gray-700 cursor-pointer"
                            >
                              CSV File
                            </label>
                          </div>
                        </div>
                      </div>
                      <div>
                        <label className="block text-sm font-medium text-gray-700 mb-1">
                          Delivery Options
                        </label>
                        <div className="mt-2 space-y-2">
                          <div className="flex items-center">
                            <input
                              id="delivery-download"
                              name="delivery-download"
                              type="checkbox"
                              className="h-4 w-4 text-indigo-600 focus:ring-indigo-500 border-gray-300 rounded cursor-pointer"
                              defaultChecked
                            />
                            <label
                              htmlFor="delivery-download"
                              className="ml-2 text-sm text-gray-700 cursor-pointer"
                            >
                              Download Immediately
                            </label>
                          </div>
                          <div className="flex items-center">
                            <input
                              id="delivery-email"
                              name="delivery-email"
                              type="checkbox"
                              className="h-4 w-4 text-indigo-600 focus:ring-indigo-500 border-gray-300 rounded cursor-pointer"
                            />
                            <label
                              htmlFor="delivery-email"
                              className="ml-2 text-sm text-gray-700 cursor-pointer"
                            >
                              Email Report
                            </label>
                          </div>
                          <div className="flex items-center">
                            <input
                              id="delivery-schedule"
                              name="delivery-schedule"
                              type="checkbox"
                              className="h-4 w-4 text-indigo-600 focus:ring-indigo-500 border-gray-300 rounded cursor-pointer"
                            />
                            <label
                              htmlFor="delivery-schedule"
                              className="ml-2 text-sm text-gray-700 cursor-pointer"
                            >
                              Schedule Recurring Report
                            </label>
                          </div>
                        </div>
                      </div>
                      <div>
                        <label className="block text-sm font-medium text-gray-700 mb-1">
                          Additional Options
                        </label>
                        <div className="mt-2 space-y-2">
                          <div className="flex items-center">
                            <input
                              id="option-cover"
                              name="option-cover"
                              type="checkbox"
                              className="h-4 w-4 text-indigo-600 focus:ring-indigo-500 border-gray-300 rounded cursor-pointer"
                              defaultChecked
                            />
                            <label
                              htmlFor="option-cover"
                              className="ml-2 text-sm text-gray-700 cursor-pointer"
                            >
                              Include Cover Page
                            </label>
                          </div>
                          <div className="flex items-center">
                            <input
                              id="option-toc"
                              name="option-toc"
                              type="checkbox"
                              className="h-4 w-4 text-indigo-600 focus:ring-indigo-500 border-gray-300 rounded cursor-pointer"
                              defaultChecked
                            />
                            <label
                              htmlFor="option-toc"
                              className="ml-2 text-sm text-gray-700 cursor-pointer"
                            >
                              Include Table of Contents
                            </label>
                          </div>
                          <div className="flex items-center">
                            <input
                              id="option-page-numbers"
                              name="option-page-numbers"
                              type="checkbox"
                              className="h-4 w-4 text-indigo-600 focus:ring-indigo-500 border-gray-300 rounded cursor-pointer"
                              defaultChecked
                            />
                            <label
                              htmlFor="option-page-numbers"
                              className="ml-2 text-sm text-gray-700 cursor-pointer"
                            >
                              Include Page Numbers
                            </label>
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
                <div className="mt-8 flex justify-end space-x-3">
                  <button
                    type="button"
                    className="inline-flex items-center px-4 py-2 border border-gray-300 text-sm font-medium rounded-md text-gray-700 bg-white hover:bg-gray-50 focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-indigo-500 !rounded-button whitespace-nowrap cursor-pointer"
                  >
                    <i className="fas fa-save mr-2"></i>
                    Save Template
                  </button>
                  <button
                    type="button"
                    className="inline-flex items-center px-4 py-2 border border-gray-300 text-sm font-medium rounded-md text-gray-700 bg-white hover:bg-gray-50 focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-indigo-500 !rounded-button whitespace-nowrap cursor-pointer"
                  >
                    <i className="fas fa-eye mr-2"></i>
                    Preview
                  </button>
                  <button
                    type="button"
                    className="inline-flex items-center px-4 py-2 border border-transparent text-sm font-medium rounded-md shadow-sm text-white bg-indigo-600 hover:bg-indigo-700 focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-indigo-500 !rounded-button whitespace-nowrap cursor-pointer"
                  >
                    <i className="fas fa-file-alt mr-2"></i>
                    Generate Report
                  </button>
                </div>
              </div>
            </div>
          )}
          {/* Placeholder for other tabs */}
          {(activeTab === "claims-analytics" ||
            activeTab === "financial" ||
            activeTab === "performance") && (
            <div className="bg-white shadow rounded-lg p-6 flex items-center justify-center h-96">
              <div className="text-center">
                <i className="fas fa-chart-line text-indigo-500 text-5xl mb-4"></i>
                <h3 className="text-lg font-medium text-gray-900 mb-2">
                  {activeTab === "claims-analytics"
                    ? "Claims Analytics"
                    : activeTab === "financial"
                      ? "Financial Reports"
                      : "Performance Metrics"}
                </h3>
                <p className="text-gray-500 max-w-md">
                  This section is under development. Please check back soon for
                  detailed{" "}
                  {activeTab === "claims-analytics"
                    ? "claims analytics"
                    : activeTab === "financial"
                      ? "financial reports"
                      : "performance metrics"}
                  .
                </p>
                <button
                  type="button"
                  className="mt-4 inline-flex items-center px-4 py-2 border border-transparent text-sm font-medium rounded-md shadow-sm text-white bg-indigo-600 hover:bg-indigo-700 focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-indigo-500 !rounded-button whitespace-nowrap cursor-pointer"
                >
                  <i className="fas fa-bell mr-2"></i>
                  Notify Me When Available
                </button>
              </div>
            </div>
          )}
        </div>
      </main>
      {/* Footer */}
      <footer className="bg-white">
        <div className="max-w-7xl mx-auto py-6 px-4 sm:px-6 lg:px-8 border-t border-gray-200">
          <div className="md:flex md:items-center md:justify-between">
            <div className="flex space-x-6 md:order-2">
              <a
                href="#"
                className="text-gray-400 hover:text-gray-500 cursor-pointer"
              >
                <span className="sr-only">Documentation</span>
                <i className="fas fa-book"></i>
              </a>
              <a
                href="#"
                className="text-gray-400 hover:text-gray-500 cursor-pointer"
              >
                <span className="sr-only">Support</span>
                <i className="fas fa-question-circle"></i>
              </a>
              <a
                href="#"
                className="text-gray-400 hover:text-gray-500 cursor-pointer"
              >
                <span className="sr-only">GitHub</span>
                <i className="fab fa-github"></i>
              </a>
            </div>
            <div className="mt-8 md:mt-0 md:order-1">
              <div className="flex items-center">
                <p className="text-sm text-gray-500">
                  &copy; 2025 Healthcare Claims Management System. All rights
                  reserved.
                </p>
                <p className="ml-6 text-sm text-gray-500">
                  <i className="fas fa-sync-alt mr-1"></i> Last synced: June 10,
                  2025 14:32
                </p>
                <p className="ml-6 text-sm text-gray-500">
                  <i className="fas fa-code-branch mr-1"></i> Version 2.4.1
                </p>
              </div>
            </div>
          </div>
        </div>
      </footer>
    </div>
  );
};
export default App;
