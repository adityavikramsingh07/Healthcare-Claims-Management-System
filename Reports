// The exported code uses Tailwind CSS. Install Tailwind CSS in your dev environment to ensure all styles work.
import React, { useState, useEffect } from "react";
import * as echarts from "echarts";
const App: React.FC = () => {
  const [activeTab, setActiveTab] = useState("overview");
  const [dateRange, setDateRange] = useState<{ start: string; end: string }>({
    start: "2025-05-01",
    end: "2025-06-10",
  });
  const [isUserMenuOpen, setIsUserMenuOpen] = useState(false);
  const [isNotificationsOpen, setIsNotificationsOpen] = useState(false);
  const [isExportDropdownOpen, setIsExportDropdownOpen] = useState(false);
  const [selectedReportTemplate, setSelectedReportTemplate] =
    useState("claims-summary");
  const [selectedExportFormat, setSelectedExportFormat] = useState("pdf");
  const [isFilterDropdownOpen, setIsFilterDropdownOpen] = useState(false);
  const [filterStatus, setFilterStatus] = useState("all");
  const [filterType, setFilterType] = useState("all");
  const [filterProvider, setFilterProvider] = useState("all");
  // Initialize charts when component mounts
  useEffect(() => {
    initCharts();
    // Clean up charts when component unmounts
    return () => {
      const charts = document.querySelectorAll(".chart-container");
      charts.forEach((chart) => {
        const instance = echarts.getInstanceByDom(chart as HTMLElement);
        if (instance) {
          echarts.dispose(instance);
        }
      });
    };
  }, [activeTab, dateRange]);
  // Initialize all charts
  const initCharts = () => {
    if (activeTab === "overview") {
      initStatusChart();
      initTrendChart();
      initAmountChart();
      initProcessingTimeChart();
    }
  };
  // Initialize status distribution chart
  const initStatusChart = () => {
    const chartDom = document.getElementById("status-chart");
    if (!chartDom) return;
    const myChart = echarts.init(chartDom);
    const option = {
      animation: false,
      tooltip: {
        trigger: "item",
        formatter: "{a} <br/>{b}: {c} ({d}%)",
      },
      legend: {
        orient: "horizontal",
        bottom: 0,
        data: ["Approved", "Pending", "Rejected"],
      },
      color: ["#10B981", "#F59E0B", "#EF4444"],
      series: [
        {
          name: "Claims Status",
          type: "pie",
          radius: ["50%", "70%"],
          avoidLabelOverlap: false,
          itemStyle: {
            borderRadius: 10,
            borderColor: "#fff",
            borderWidth: 2,
          },
          label: {
            show: false,
            position: "center",
          },
          emphasis: {
            label: {
              show: true,
              fontSize: "18",
              fontWeight: "bold",
            },
          },
          labelLine: {
            show: false,
          },
          data: [
            { value: 485, name: "Approved" },
            { value: 210, name: "Pending" },
            { value: 105, name: "Rejected" },
          ],
        },
      ],
    };
    option && myChart.setOption(option);
  };
  // Initialize claims trend chart
  const initTrendChart = () => {
    const chartDom = document.getElementById("trend-chart");
    if (!chartDom) return;
    const myChart = echarts.init(chartDom);
    const option = {
      animation: false,
      tooltip: {
        trigger: "axis",
      },
      legend: {
        data: ["Total Claims", "Approved", "Pending", "Rejected"],
        bottom: 0,
      },
      grid: {
        left: "3%",
        right: "4%",
        bottom: "15%",
        top: "3%",
        containLabel: true,
      },
      xAxis: {
        type: "category",
        boundaryGap: false,
        data: [
          "May 1",
          "May 8",
          "May 15",
          "May 22",
          "May 29",
          "Jun 5",
          "Jun 10",
        ],
      },
      yAxis: {
        type: "value",
      },
      series: [
        {
          name: "Total Claims",
          type: "line",
          stack: "Total",
          data: [120, 132, 101, 134, 90, 230, 210],
          smooth: true,
          lineStyle: {
            width: 3,
            color: "#6366F1",
          },
          itemStyle: {
            color: "#6366F1",
          },
        },
        {
          name: "Approved",
          type: "line",
          stack: "Status",
          data: [80, 92, 71, 84, 50, 130, 110],
          smooth: true,
          lineStyle: {
            width: 2,
            color: "#10B981",
          },
          itemStyle: {
            color: "#10B981",
          },
        },
        {
          name: "Pending",
          type: "line",
          stack: "Status",
          data: [30, 32, 21, 34, 30, 70, 80],
          smooth: true,
          lineStyle: {
            width: 2,
            color: "#F59E0B",
          },
          itemStyle: {
            color: "#F59E0B",
          },
        },
        {
          name: "Rejected",
          type: "line",
          stack: "Status",
          data: [10, 8, 9, 16, 10, 30, 20],
          smooth: true,
          lineStyle: {
            width: 2,
            color: "#EF4444",
          },
          itemStyle: {
            color: "#EF4444",
          },
        },
      ],
    };
    option && myChart.setOption(option);
  };
  // Initialize amount distribution chart
  const initAmountChart = () => {
    const chartDom = document.getElementById("amount-chart");
    if (!chartDom) return;
    const myChart = echarts.init(chartDom);
    const option = {
      animation: false,
      tooltip: {
        trigger: "axis",
        axisPointer: {
          type: "shadow",
        },
      },
      grid: {
        left: "3%",
        right: "4%",
        bottom: "15%",
        top: "3%",
        containLabel: true,
      },
      xAxis: [
        {
          type: "category",
          data: [
            "Medical",
            "Surgical",
            "Dental",
            "Vision",
            "Diagnostic",
            "Preventive",
            "Other",
          ],
          axisTick: {
            alignWithLabel: true,
          },
        },
      ],
      yAxis: [
        {
          type: "value",
          name: "Amount ($)",
        },
      ],
      series: [
        {
          name: "Total Amount",
          type: "bar",
          barWidth: "60%",
          data: [
            { value: 58000, itemStyle: { color: "#6366F1" } },
            { value: 42000, itemStyle: { color: "#8B5CF6" } },
            { value: 25000, itemStyle: { color: "#EC4899" } },
            { value: 18000, itemStyle: { color: "#F97316" } },
            { value: 35000, itemStyle: { color: "#14B8A6" } },
            { value: 22000, itemStyle: { color: "#84CC16" } },
            { value: 15000, itemStyle: { color: "#9CA3AF" } },
          ],
        },
      ],
    };
    option && myChart.setOption(option);
  };
  // Initialize processing time chart
  const initProcessingTimeChart = () => {
    const chartDom = document.getElementById("processing-time-chart");
    if (!chartDom) return;
    const myChart = echarts.init(chartDom);
    // Generate some scatter data
    const data = [];
    for (let i = 0; i < 50; i++) {
      const claimSize = Math.random() * 4000 + 1000;
      const processingTime = Math.random() * 72 + 12;
      const status =
        Math.random() > 0.7
          ? "rejected"
          : Math.random() > 0.4
            ? "pending"
            : "approved";
      let color;
      if (status === "approved") color = "#10B981";
      else if (status === "pending") color = "#F59E0B";
      else color = "#EF4444";
      data.push([claimSize, processingTime, status, color]);
    }
    const option = {
      animation: false,
      grid: {
        left: "3%",
        right: "4%",
        bottom: "15%",
        top: "10%",
        containLabel: true,
      },
      tooltip: {
        trigger: "item",
        formatter: function (params: any) {
          return `Claim Amount: $${params.value[0].toFixed(2)}<br/>Processing Time: ${params.value[1].toFixed(1)} hours<br/>Status: ${params.value[2]}`;
        },
      },
      xAxis: {
        name: "Claim Amount ($)",
        nameLocation: "middle",
        nameGap: 30,
        type: "value",
      },
      yAxis: {
        name: "Processing Time (hours)",
        nameLocation: "middle",
        nameGap: 30,
        type: "value",
      },
      legend: {
        data: ["Approved", "Pending", "Rejected"],
        bottom: 0,
      },
      series: [
        {
          name: "Approved",
          type: "scatter",
          symbolSize: 10,
          data: data
            .filter((item) => item[2] === "approved")
            .map((item) => [item[0], item[1]]),
          itemStyle: {
            color: "#10B981",
          },
        },
        {
          name: "Pending",
          type: "scatter",
          symbolSize: 10,
          data: data
            .filter((item) => item[2] === "pending")
            .map((item) => [item[0], item[1]]),
          itemStyle: {
            color: "#F59E0B",
          },
        },
        {
          name: "Rejected",
          type: "scatter",
          symbolSize: 10,
          data: data
            .filter((item) => item[2] === "rejected")
            .map((item) => [item[0], item[1]]),
          itemStyle: {
            color: "#EF4444",
          },
        },
      ],
    };
    option && myChart.setOption(option);
  };
  // Handle date range change
  const handleDateRangeChange = (
    e: React.ChangeEvent<HTMLInputElement>,
    type: "start" | "end",
  ) => {
    setDateRange((prev) => ({
      ...prev,
      [type]: e.target.value,
    }));
  };
  return (
    <div className="min-h-screen bg-gray-50">
      {/* Header */}
      <header className="bg-white shadow-sm fixed top-0 left-0 right-0 z-10">
        <div className="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
          <div className="flex justify-between h-16">
            <div className="flex items-center">
              <div className="flex-shrink-0 flex items-center">
                <img
                  className="h-8 w-auto"
                  src="https://readdy.ai/api/search-image?query=A%252520modern%25252C%252520minimalist%252520healthcare%252520logo%252520with%252520a%252520blue%252520and%252520teal%252520color%252520scheme%25252C%252520featuring%252520a%252520stylized%252520medical%252520cross%252520or%252520caduceus%252520symbol%252520integrated%252520with%252520a%252520shield%252520or%252520protective%252520element%25252C%252520suitable%252520for%252520a%252520healthcare%252520claims%252520management%252520system&width=200&height=80&seq=logo1&orientation=landscape"
                  alt="Healthcare Claims Management"
                />
                <span className="ml-2 text-lg font-semibold text-gray-900">
                  ClaimsPro
                </span>
              </div>
              <nav className="ml-6 flex space-x-8">
                <a
                  href="https://readdy.ai/home/05c949dc-8c27-4326-acdb-c32485d3bc67/3c98454b-7c94-4c48-a77d-0774227525af"
                  data-readdy="true"
                  className="border-transparent text-gray-500 hover:border-gray-300 hover:text-gray-700 inline-flex items-center px-1 pt-1 border-b-2 text-sm font-medium cursor-pointer"
                >
                  Dashboard
                </a>
                <a
                  href="https://readdy.ai/home/05c949dc-8c27-4326-acdb-c32485d3bc67/55e4f658-5d14-4dcd-bd63-ca1bbc2dfe9c"
                  data-readdy="true"
                  className="border-transparent text-gray-500 hover:border-gray-300 hover:text-gray-700 inline-flex items-center px-1 pt-1 border-b-2 text-sm font-medium cursor-pointer"
                >
                  Claims
                </a>
                <a
                  href="#"
                  className="border-b-2 border-indigo-500 text-gray-900 inline-flex items-center px-1 pt-1 text-sm font-medium cursor-pointer"
                >
                  Reports
                </a>
                <a
                  href="https://readdy.ai/home/05c949dc-8c27-4326-acdb-c32485d3bc67/14175283-a725-4f0b-8576-d1fd3f13cd28"
                  data-readdy="true"
                  className="border-transparent text-gray-500 hover:border-gray-300 hover:text-gray-700 inline-flex items-center px-1 pt-1 border-b-2 text-sm font-medium cursor-pointer"
                >
                  Settings
                </a>
              </nav>
            </div>
            <div className="flex items-center">
              <div className="w-full max-w-lg lg:max-w-xs relative">
                <label htmlFor="search" className="sr-only">
                  Search
                </label>
                <div className="relative">
                  <div className="absolute inset-y-0 left-0 pl-3 flex items-center pointer-events-none">
                    <i className="fas fa-search text-gray-400 text-sm"></i>
                  </div>
                  <input
                    id="search"
                    name="search"
                    className="block w-full pl-10 pr-3 py-2 border border-gray-300 rounded-md leading-5 bg-white placeholder-gray-500 focus:outline-none focus:placeholder-gray-400 focus:ring-1 focus:ring-indigo-500 focus:border-indigo-500 sm:text-sm"
                    placeholder="Search reports, metrics, analytics..."
                    type="search"
                  />
                </div>
              </div>
              <div className="ml-4 flex items-center md:ml-6">
                {/* Notifications dropdown */}
                <div className="relative">
                  <button
                    type="button"
                    className="bg-white p-1 rounded-full text-gray-400 hover:text-gray-500 focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-indigo-500 !rounded-button whitespace-nowrap cursor-pointer"
                    onClick={() => setIsNotificationsOpen(!isNotificationsOpen)}
                  >
                    <span className="sr-only">View notifications</span>
                    <i className="fas fa-bell"></i>
                    <span className="absolute top-0 right-0 block h-2 w-2 rounded-full bg-red-500"></span>
                  </button>
                  {isNotificationsOpen && (
                    <div className="origin-top-right absolute right-0 mt-2 w-80 rounded-md shadow-lg py-1 bg-white ring-1 ring-black ring-opacity-5 focus:outline-none z-20">
                      <div className="px-4 py-2 border-b border-gray-100">
                        <h3 className="text-sm font-medium text-gray-900">
                          Notifications
                        </h3>
                      </div>
                      <div className="max-h-60 overflow-y-auto">
                        <a
                          href="#"
                          className="block px-4 py-2 text-sm text-gray-700 hover:bg-gray-100 cursor-pointer"
                        >
                          <p className="font-medium">Monthly report ready</p>
                          <p className="text-xs text-gray-500">
                            May 2025 Claims Report - 10 minutes ago
                          </p>
                        </a>
                        <a
                          href="#"
                          className="block px-4 py-2 text-sm text-gray-700 hover:bg-gray-100 cursor-pointer"
                        >
                          <p className="font-medium">Analytics updated</p>
                          <p className="text-xs text-gray-500">
                            Dashboard metrics refreshed - 1 hour ago
                          </p>
                        </a>
                        <a
                          href="#"
                          className="block px-4 py-2 text-sm text-gray-700 hover:bg-gray-100 cursor-pointer"
                        >
                          <p className="font-medium">
                            Scheduled report completed
                          </p>
                          <p className="text-xs text-gray-500">
                            Weekly claims summary - 2 hours ago
                          </p>
                        </a>
                      </div>
                      <div className="px-4 py-2 border-t border-gray-100 text-center">
                        <a
                          href="#"
                          className="text-xs text-indigo-600 hover:text-indigo-500 cursor-pointer"
                        >
                          View all notifications
                        </a>
                      </div>
                    </div>
                  )}
                </div>
                {/* Profile dropdown */}
                <div className="ml-3 relative">
                  <div>
                    <button
                      type="button"
                      className="max-w-xs bg-white flex items-center text-sm rounded-full focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-indigo-500 !rounded-button whitespace-nowrap cursor-pointer"
                      onClick={() => setIsUserMenuOpen(!isUserMenuOpen)}
                    >
                      <span className="sr-only">Open user menu</span>
                      <img
                        className="h-8 w-8 rounded-full"
                        src="https://readdy.ai/api/search-image?query=Professional%252520headshot%252520of%252520a%252520healthcare%252520administrator%25252C%252520business%252520professional%252520with%252520a%252520friendly%252520expression%25252C%252520wearing%252520business%252520attire%25252C%252520neutral%252520background%25252C%252520high%252520quality%252520portrait&width=100&height=100&seq=user1&orientation=squarish"
                        alt="User profile"
                      />
                    </button>
                  </div>
                  {isUserMenuOpen && (
                    <div className="origin-top-right absolute right-0 mt-2 w-48 rounded-md shadow-lg py-1 bg-white ring-1 ring-black ring-opacity-5 focus:outline-none z-20">
                      <a
                        href="#"
                        className="block px-4 py-2 text-sm text-gray-700 hover:bg-gray-100 cursor-pointer"
                      >
                        Your Profile
                      </a>
                      <a
                        href="#"
                        className="block px-4 py-2 text-sm text-gray-700 hover:bg-gray-100 cursor-pointer"
                      >
                        Settings
                      </a>
                      <a
                        href="#"
                        className="block px-4 py-2 text-sm text-gray-700 hover:bg-gray-100 cursor-pointer"
                      >
                        Sign out
                      </a>
                    </div>
                  )}
                </div>
              </div>
            </div>
          </div>
        </div>
      </header>
      <main className="pt-16 pb-10 min-h-screen">
        <div className="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
          {/* Page Header with Breadcrumbs */}
          <div className="py-6">
            <div className="flex items-center text-sm text-gray-500 mb-4">
              <a
                href="https://readdy.ai/home/05c949dc-8c27-4326-acdb-c32485d3bc67/3c98454b-7c94-4c48-a77d-0774227525af"
                data-readdy="true"
                className="hover:text-indigo-600 cursor-pointer"
              >
                Dashboard
              </a>
              <i className="fas fa-chevron-right mx-2 text-xs"></i>
              <span className="text-gray-700">Reports & Analytics</span>
            </div>
            <div className="flex justify-between items-center">
              <div>
                <h1 className="text-2xl font-semibold text-gray-900">
                  Reports & Analytics
                </h1>
                <p className="mt-1 text-sm text-gray-500">
                  Analyze claims data, generate reports, and visualize key
                  metrics.
                </p>
              </div>
              <div className="flex space-x-3">
                <div className="relative">
                  <button
                    type="button"
                    className="inline-flex items-center px-4 py-2 border border-gray-300 text-sm font-medium rounded-md text-gray-700 bg-white hover:bg-gray-50 focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-indigo-500 !rounded-button whitespace-nowrap cursor-pointer"
                    onClick={() =>
                      setIsExportDropdownOpen(!isExportDropdownOpen)
                    }
                  >
                    <i className="fas fa-download mr-2"></i>
                    Export Report
                    <i className="fas fa-chevron-down ml-2"></i>
                  </button>
                  {isExportDropdownOpen && (
                    <div className="origin-top-right absolute right-0 mt-2 w-56 rounded-md shadow-lg bg-white ring-1 ring-black ring-opacity-5 focus:outline-none z-10">
                      <div
                        className="py-1"
                        role="menu"
                        aria-orientation="vertical"
                      >
                        <button
                          className="text-gray-700 block px-4 py-2 text-sm w-full text-left hover:bg-gray-100 cursor-pointer"
                          onClick={() => setSelectedExportFormat("pdf")}
                        >
                          <i className="fas fa-file-pdf mr-2 text-red-600"></i>
                          Export to PDF
                        </button>
                        <button
                          className="text-gray-700 block px-4 py-2 text-sm w-full text-left hover:bg-gray-100 cursor-pointer"
                          onClick={() => setSelectedExportFormat("excel")}
                        >
                          <i className="fas fa-file-excel mr-2 text-green-600"></i>
                          Export to Excel
                        </button>
                        <button
                          className="text-gray-700 block px-4 py-2 text-sm w-full text-left hover:bg-gray-100 cursor-pointer"
                          onClick={() => setSelectedExportFormat("csv")}
                        >
                          <i className="fas fa-file-csv mr-2 text-blue-600"></i>
                          Export to CSV
                        </button>
                        <div className="border-t border-gray-100 my-1"></div>
                        <button className="text-gray-700 block px-4 py-2 text-sm w-full text-left hover:bg-gray-100 cursor-pointer">
                          <i className="fas fa-envelope mr-2 text-indigo-600"></i>
                          Email Report
                        </button>
                        <button className="text-gray-700 block px-4 py-2 text-sm w-full text-left hover:bg-gray-100 cursor-pointer">
                          <i className="fas fa-print mr-2 text-gray-600"></i>
                          Print Report
                        </button>
                      </div>
                    </div>
                  )}
                </div>
                <button
                  type="button"
                  className="inline-flex items-center px-4 py-2 border border-transparent text-sm font-medium rounded-md shadow-sm text-white bg-indigo-600 hover:bg-indigo-700 focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-indigo-500 !rounded-button whitespace-nowrap cursor-pointer"
                >
                  <i className="fas fa-plus mr-2"></i>
                  New Report
                </button>
              </div>
            </div>
          </div>
          {/* Report Tabs */}
          <div className="bg-white shadow rounded-lg mb-6">
            <div className="border-b border-gray-200">
              <nav className="-mb-px flex space-x-8 px-6" aria-label="Tabs">
                <button
                  onClick={() => setActiveTab("overview")}
                  className={`${
                    activeTab === "overview"
                      ? "border-indigo-500 text-indigo-600"
                      : "border-transparent text-gray-500 hover:text-gray-700 hover:border-gray-300"
                  } whitespace-nowrap py-4 px-1 border-b-2 font-medium text-sm cursor-pointer`}
                >
                  <i className="fas fa-chart-pie mr-2"></i>
                  Dashboard Overview
                </button>
                <button
                  onClick={() => setActiveTab("claims-analytics")}
                  className={`${
                    activeTab === "claims-analytics"
                      ? "border-indigo-500 text-indigo-600"
                      : "border-transparent text-gray-500 hover:text-gray-700 hover:border-gray-300"
                  } whitespace-nowrap py-4 px-1 border-b-2 font-medium text-sm cursor-pointer`}
                >
                  <i className="fas fa-chart-line mr-2"></i>
                  Claims Analytics
                </button>
                <button
                  onClick={() => setActiveTab("financial")}
                  className={`${
                    activeTab === "financial"
                      ? "border-indigo-500 text-indigo-600"
                      : "border-transparent text-gray-500 hover:text-gray-700 hover:border-gray-300"
                  } whitespace-nowrap py-4 px-1 border-b-2 font-medium text-sm cursor-pointer`}
                >
                  <i className="fas fa-dollar-sign mr-2"></i>
                  Financial Reports
                </button>
                <button
                  onClick={() => setActiveTab("performance")}
                  className={`${
                    activeTab === "performance"
                      ? "border-indigo-500 text-indigo-600"
                      : "border-transparent text-gray-500 hover:text-gray-700 hover:border-gray-300"
                  } whitespace-nowrap py-4 px-1 border-b-2 font-medium text-sm cursor-pointer`}
                >
                  <i className="fas fa-tachometer-alt mr-2"></i>
                  Performance Metrics
                </button>
                <button
                  onClick={() => setActiveTab("custom")}
                  className={`${
                    activeTab === "custom"
                      ? "border-indigo-500 text-indigo-600"
                      : "border-transparent text-gray-500 hover:text-gray-700 hover:border-gray-300"
                  } whitespace-nowrap py-4 px-1 border-b-2 font-medium text-sm cursor-pointer`}
                >
                  <i className="fas fa-cog mr-2"></i>
                  Custom Reports
                </button>
              </nav>
            </div>
          </div>
          {/* Filters and Date Range */}
          <div className="bg-white shadow rounded-lg mb-6">
            <div className="px-4 py-5 sm:p-6">
